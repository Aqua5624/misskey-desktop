{
  "name": "misskey-desktop",
  "version": "1.0.0",
  "description": "Misskey Desktop unofficial desktop client",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "electronDownload": {
      "cache": "cache"
    },
    "appId": "com.troke12.misskey",
    "productName": "Misskey Desktop",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.social-networking",
      "icon": "assets/512x512.png"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/256x256.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/256x256.png"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0"
  }
}
